<%- include("./partials/header") %>
<%- include("./navbar/menu") %>

<br>

<div class="container">
  <h5 class="border-bottom pb-4 mb-2">Exams Types</h5>
  <br>
    <div class="row g-2">

        <% if (typeof exams != 'undefined') { %> <% exams.forEach(exam => { %>

          <div class="col-4">
            <div class="p-5 border bg-dark text-danger rounded-3 mb-4">
                <p><strong>Exam Type Code: </strong><i class="text-white"><%= exam.id %></i></p>
                <p><strong>Exam Type Name: </strong><i class="text-white"><%= exam.name %></i></p>
                <p><strong>Exam Type Gravity: </strong><i class="text-white"><%= exam.gravity %></i></p>
            </div>
          </div>

      <% }) %> <% } %>

    </div>
  </div>

<br>

<%- include("./partials/footer") %>


